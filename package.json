{
  "name": "my-app",
  "version": "0.1.0",
  "scripts": {
    "start": "npm run start:editor || npm run start:renderer",
    "setup": "npm run setup:editor && npm run setup:renderer",
    "start:editor": "cd apps/editor && npm run start",
    "setup:editor": "cd apps/editor && npm run setup",
    "start:renderer": "cd apps/renderer && npm run start",
    "setup:renderer": "cd apps/renderer && npm run setup"
  },
  "devDependencies": {
    "npm-run-all": "^4.1.5"
  }
}
